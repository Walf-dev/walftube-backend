{
  "name": "walftube-backend",
  "version": "1.0.0",
  "description": "walftube backend using Express + Sequelize",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server",
    "dev": "nodemon src/server",
    "begin": "npm run build && node src/server.js",
    "start:original": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node src/server.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build"
  },
  "engines": {
    "node": "14.18.1"
  },
  "author": "Fredoly Wagni",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.2.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
